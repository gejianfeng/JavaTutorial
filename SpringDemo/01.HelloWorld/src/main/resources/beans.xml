<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="springHelloWorld" class="com.gejianfeng.springdemo.helloworld1.impl.SpringHelloWorld"></bean>
	<bean id="strutsHelloWorld" class="com.gejianfeng.springdemo.helloworld1.impl.StrutsHelloWorld"></bean>
	
	<bean id="helloWorldService" class="com.gejianfeng.springdemo.helloworld1.services.HelloWorldService">
		<property name="helloWorld" ref="springHelloWorld"/>
	</bean>
</beans>